{
  "name": "@cfware/queue_log-mysql",
  "version": "0.1.0",
  "description": "CFWare queue_log mysql writer",
  "main": "index.js",
  "scripts": {
    "test": "eslint *.js test/*.js && nyc mocha"
  },
  "engines": {
    "nodejs": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfware/queue_log-mysql.git"
  },
  "author": "Corey Farrell",
  "license": "MIT",
  "keywords": [
    "asterisk",
    "queue_log",
    "queuemetrics",
    "mysql"
  ],
  "files": [
    "*.js",
    ".eslintrc.json",
    "sampledb",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/cfware/queue_log-mysql/issues"
  },
  "homepage": "https://github.com/cfware/queue_log-mysql#readme",
